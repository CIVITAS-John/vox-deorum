{
  "agent": {
    "name": "vox-agents",
    "version": "1.0.0"
  },
  "mcpServer": {
    "transport": {
      "type": "http",
      "endpoint": "http://127.0.0.1:4000/mcp"
    }
  },
  "logging": {
    "level": "info"
  },
  "llms": {
    "default": "gpt-oss-20b",
    "gemma3-27b": {
      "provider": "openrouter",
      "name": "google/gemma-3-27b-it",
      "options": {
        "toolMiddleware": "gemma"
      }
    },
    "gpt-5-mini-minimal": {
      "provider": "openai",
      "name": "gpt-5-mini",
      "options": {
        "reasoningEffort": "minimal"
      }
    },
    "gpt-5-nano-minimal": {
      "provider": "openai",
      "name": "gpt-5-nano",
      "options": {
        "reasoningEffort": "minimal"
      }
    },
    "gpt-oss-20b": {
      "provider": "openrouter",
      "name": "openai/gpt-oss-20b",
      "options": {
        "toolMiddleware": "gemma"
      }
    },
    "gpt-oss-20b-minimal": {
      "provider": "openrouter",
      "name": "openai/gpt-oss-20b",
      "options": {
        "reasoningEffort": "minimal",
        "toolMiddleware": "gemma"
      }
    },
    "chutes/gpt-oss-120b": {
      "provider": "chutes",
      "name": "openai/gpt-oss-120b",
      "options": {
        "toolMiddleware": "gemma"
      }
    },
    "chutes/glm-4.6": {
      "provider": "chutes",
      "name": "zai-org/GLM-4.6-FP8"
    },
    "jetstream2/gpt-oss-120b": {
      "provider": "jetstream2",
      "name": "gpt-oss-120b",
      "options": {
        "toolMiddleware": "gemma"
      }
    },
    "jetstream2/gpt-oss-120b-minimal": {
      "provider": "jetstream2",
      "name": "gpt-oss-120b",
      "options": {
        "reasoningEffort": "minimal",
        "toolMiddleware": "gemma"
      }
    }
  }
} 