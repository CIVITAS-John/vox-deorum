{
  "agent": {
    "name": "vox-agents",
    "version": "1.0.0"
  },
  "mcpServer": {
    "transport": {
      "type": "http",
      "endpoint": "http://127.0.0.1:4000/mcp"
    }
  },
  "logging": {
    "level": "info"
  },
  "llms": {
    "default": "gemma3",
    "gemma3": {
      "provider": "openrouter",
      "name": "google/gemma-3-27b-it"
    },
    "gpt-5-mini-minimal": {
      "provider": "openai",
      "name": "gpt-5-mini",
      "options": {
        "reasoningEffort": "minimal"
      }
    }
  }
} 